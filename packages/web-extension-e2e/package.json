{
  "name": "web-extension-e2e",
  "version": "0.0.0",
  "description": "",
  "author": "",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "lint": "eslint --cache --ext .ts,.js,.mjs,.cjs . && prettier -c e2e lib",
    "lint:fix": "eslint --cache --ext .ts,.js,.mjs,.cjs . --fix && prettier -c e2e lib -w",
    "test": "pnpm lint && pnpm test:types",
    "test:types": "tsc --noEmit --skipLibCheck",
    "pree2e": "pnpm --filter web-extension build",
    "e2e": "playwright test",
    "pree2e:ui": "pnpm run pree2e",
    "e2e:ui": "playwright test --ui",
    "e2e:report": "playwright show-report",
    "e2e:codegen": "playwright codegen"
  },
  "devDependencies": {
    "@playwright/test": "^1.46.1",
    "@types/node": "^20.16.1",
    "eslint": "^8.47.0",
    "eslint-config-dsfx3d": "^0.1.2",
    "prettier": "^3.0.2",
    "typescript": "^5.1.6",
    "web-extension": "workspace:*"
  }
}
